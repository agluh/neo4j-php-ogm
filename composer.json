{
    "name": "longitude-one/neo4j-php-ogm",
    "description": "PHP Object Graph Mapper for Neo4j",
    "license": "MIT",
    "keywords": [
        "neo4j",
        "graph",
        "ogm",
        "mapper",
        "mapping",
        "annotations",
        "database",
        "cypher"
    ],
    "homepage": "https://github.com/graphaware/neo4j-php-ogm",
    "authors": [{
        "name": "Christophe Willemsen",
        "email": "christophe@graphaware.com"
    }, {
        "name": "GraphAware Ltd",
        "email": "php@graphaware.com"
    }, {
        "name": "Alexandre Tranchant",
        "email": "alexandre.tranchant@gmail.com"
    }],
    "require": {
        "php": "^7.2",
        "doctrine/annotations": "^1.2",
        "doctrine/collections": "^1.3",
        "doctrine/common": "^2.6",
        "longitude-one/neo4j-php-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GraphAware\\Neo4j\\OGM\\": "src/"
        },
        "files": ["src/Mapping/Neo4jOGMAnnotations.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Neo4j\\OGM\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "symfony/finder": "^4.4 || ^5.0",
        "symfony/stopwatch": "^4.4 || ^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
